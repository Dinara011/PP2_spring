   "source": [
    "Write a Python program that matches a string that has an `'a'` followed by two to three `'b'`."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "a5de35f5-dd4c-437a-bfe8-831fdcc20b6c",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " aaabb\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "False\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "text = input()\n",
    "pattern = r\"ab{2,3}$\" \n",
    "print(bool(re.fullmatch(pattern, text)))"
   ]
  },
